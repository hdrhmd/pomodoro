{
    "name": "pomodoro",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Pomodoro"
    },
    "permissions": [
      "storage"
    ],
    "background": {
      "scripts": [
        "/lib/js/moment.min.js",
        "background.js"
      ]
    }
  }